# Ethane Gas: polymer builder on CCC via -grid; lammps simulation; density
run_nemd polymer_builder_driver.py *C* -cru_num 3 -cell grid -mol_num 200 -seed 5678 -temp 298.15 -prod_ens NPT -prod_time 10
lmp_serial < polymer_builder.in | tee lammps.log
run_nemd custom_dump_driver.py dump.custom.gz -task xyz -data_file polymer_builder.data -task density
grep "g/cm^3" custom_dump-driver.log
# 1.808 kg/m3 at 25 C https://en.wikipedia.org/wiki/Propane#Density
